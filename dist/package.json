{
    "version": "1.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rm -rf ./dist && tsc && cp -R package*.json .dockerignore docker-compose.yml Dockerfile *Images.sh src/public dist/ && cp .env.prod dist/.env",
        "lint": "eslint \"src\"",
        "start": "dotenv -c '.env' -- node dist/app",
        "publish": "./build.sh",
        "backupImages": "./backupImages.sh",
        "restoreImages": "./backupImages.sh",
        "minor": "npm version minor && node update-env.js",
        "major": "npm version major && node update-env.js",
        "patch": "npm version patch && node update-env.js"
    },
    "dependencies": {
        "@adminjs/express": "^6.1.0",
        "@adminjs/mongoose": "^4.1.0",
        "@adminjs/upload": "^4.0.2",
        "adminjs": "^7.7.2",
        "bcrypt": "^5.1.1",
        "connect-mongo": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "express-formidable": "^1.2.0",
        "express-session": "^1.17.3",
        "fs": "^0.0.1-security",
        "mongoose": "^8.0.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.17",
        "@types/node": "^18.15.11",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "dotenv-cli": "^7.2.1",
        "eslint": "^8.38.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.1.0",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4"
    },
    "name": "bridge"
}
